public class Node<E>{
 
    private E data;
    private int x,y;
    private Node<E> next;

    // no <E> in constructor
    public Node(){
	data = null;
	next = null;
    }

    public Node(E s){
	data = s;
	next = null;
    }

    public void setCor(int xcor, int ycor){
	x = xcor;
	y = ycor;
    }

    public int getX(){
	return x;
    }

    public int getY(){
	return y;
    }
    
    public void setData(E s){
	data = s;
    }

    public E getData(){
	return data;
    }

    public void setNext(Node<E> n){
	next = n;
    }

    public Node<E> getNext(){
	return next;
    }

    public String toString(){
	return ""+data;
    }

}
